!=================!
! Xresources file !
!=================!

!=========!
!  URxvt* !
!=========!

! = Functionality = !
!URxvt*depth: 32

!--------------!
! Key Bindings !
!--------------!

URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~

!------!
! Perl !
!------!

URxvt*perl-ext-common: default

!---------------------------!
! Transparency / Background !
!---------------------------!


URxvt*transparent: 	false
!URxvt*shading: 		10
URxvt*background:	#2D2D2D
URxvt*foreground:	#F9F9F9
URxvt*pointerColor:	black
URxvt*pointerColor2:	white
URxvt*borderless:	true
URxvt*colorMode: 	on
URxvt*cursorBlink: 	true
URxvt*internalBorder:	7

!-----------!
! Scrollbar !
!-----------!

URxvt*scrollBar:	false


!------!
! Font !
!------!

URxvt*font:		xft:GohuFont:size=11
URxvt*antialias:	false
!URxvt*tintColor:	white

!--------------!
! Xft Settings !
!--------------!

Xft.dpi: 		66
Xft.antialias: 		true
Xft.rgba: 		rgb
Xft.autohint: 		true
Xft.hinting: 		true
Xft.hintstyle: 		hintfull
Xft.lcdfilter: 		lcddefault


